        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CalculationNumericalSeries"
        mc:Ignorable="d"
        Title="Подсказки" Height="179.237" Width="305.509" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen">
    <Grid>
        <ListBox Margin="10" HorizontalContentAlignment="Center">
            <ListBoxItem>sin(x) – функция cинуса от x</ListBoxItem>
            <ListBoxItem>cos(x) – функция косинуса от x</ListBoxItem>
            <ListBoxItem>tg(x) – функция тангенса от x</ListBoxItem>
