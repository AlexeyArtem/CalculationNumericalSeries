<Window x:Class="CalculationNumericalSeries.PromptingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CalculationNumericalSeries"
        mc:Ignorable="d"
        Title="Подсказки" Height="179.237" Width="305.509" ResizeMode="CanMinimize" WindowStartupLocation="CenterScreen">
    <Grid>
        <ListBox Margin="10" HorizontalContentAlignment="Center">
            <ListBoxItem>sin(n) – функция cинуса от n</ListBoxItem>
            <ListBoxItem>cos(n) – функция косинуса от n</ListBoxItem>
            <ListBoxItem>tg(n) – функция тангенса от n</ListBoxItem>
            <ListBoxItem>absolute(n) – модуль n</ListBoxItem>
            <ListBoxItem>ln(n) – натуральный логарифм от n</ListBoxItem>
            <ListBoxItem>lg(n) – десятичный логарифм от n</ListBoxItem>
            <ListBoxItem>sqrt(n) – функция квадратного корня от n</ListBoxItem>
        </ListBox>
    </Grid>
</Window>
