<Window x:Class="CalculationNumericalSeries.AddProjectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CalculationNumericalSeries"
        mc:Ignorable="d"
        Title="Добавление проекта" Height="160.245" Width="285.5" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.5*"></RowDefinition>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Label Grid.Row="0" HorizontalAlignment="Center" VerticalAlignment="Bottom">Название проекта:</Label>
        <TextBox x:Name="TbNameProject" Grid.Row="1" Style="{StaticResource ResourceKey=DataInputElement}"></TextBox>
        <Button x:Name="BtAddProject" Grid.Row="2" Style="{StaticResource ResourceKey=ControlButton}" Click="BtAddProject_Click">Добавить</Button>
    </Grid>
</Window>
